olumn":36}},{"generated":{"line":90,"column":35},"source":"app_1.js","original":{"line":88,"column":12}},{"generated":{"line":91,"column":0},"source":"app_1.js","original":{"line":90,"column":9}},{"generated":{"line":92,"column":0},"source":"app_1.js","original":{"line":91,"column":5}},{"generated":{"line":93,"column":0},"source":"app_1.js","original":{"line":50,"column":10}},{"generated":{"line":93,"column":1},"source":"app_1.js","original":{"line":50,"column":0}},{"name":"c","generated":{"line":94,"column":15},"source":"app_1.js","original":{"line":94,"column":15}}],"sources":{"app_1.js":"import './app_1.css'\nimport $ from 'jquery'\n// console.log($);\n\nconst eventBus = $(window)\n// console.log(eventBus.on);\n// console.log(eventBus.trigger);\n\n// 数据相关都放到M\nconst m = {\n    data: {\n        n : parseInt(localStorage.getItem('n'))\n    },\n    create(){},\n    delete(){},\n    update(data){\n        Object.assign(m.data, data)\n        eventBus.trigger('m_updated')\n        localStorage.setItem('n', m.data.n)\n    },\n    get(){}\n}\n// 视图相关都放到V\nconst v = {\n    el: null,\n    html: `\n    <!-- 有数据存储功能的计算器 -->\n    <div>\n        <div class=\"output\">\n            <span id=\"number\">{{n}}</span>\n        </div>\n        <div class=\"actions\">\n            <button id=\"add_1\">+1</button>\n            <button id=\"minus_1\">-1</button>\n            <button id=\"mul_1\">*2</button>\n            <button id=\"divide_1\">/2</button>\n        </div>\n    </div>\n    `,\n    init(container) {\n        v.el = $(container)\n    },\n    render(n){\n        if (v.el.children.length !== 0) v.el.empty()\n        $(v.html.replace('{{n}}', n))\n            .appendTo(v.el)\n    }\n}\n// 其他的都放到C\nconst c = {\n    init(container) {\n        v.init(container)\n        v.render(m.data.n)\n        c.autoBindEvents()\n        eventBus.on('m_updated', ()=> {\n            v.render(m.data.n)\n        })\n    },\n    events: {\n        'click #add_1': 'add',\n        'click #minus_1': 'minus',\n        'click #mul_1': 'mul',\n        'click #divide_1': 'divide'\n    },\n    add() {\n        m.update({n: m.data.n + 1})\n    },\n    minus() {\n        m.update({n: m.data.n - 1})\n    },\n    mul() {\n        m.update({n: m.data.n * 2})\n    },\n    divide() {\n        m.update({n: m.data.n / 2})\n    },\n\n    autoBindEvents() {\n        for (let key in c.events) {\n            const value = c[c.events[key]]\n            // console.log(value);\n            const spaceIndex = key.indexOf(' ')\n            const part_1 = key.slice(0, spaceIndex)\n            const part_2 = key.slice(spaceIndex + 1)\n            // console.log(part_1);\n            // console.log(part_2);\n            // console.log(part_1, part_2, value)\n            v.el.on(part_1, part_2, value)\n\n        }\n    }\n}\n\nexport default c"},"lineCount":null}},"error":null,"hash":"89e974274329b7ec7de4f92095039338","cacheData":{"env":{}}}