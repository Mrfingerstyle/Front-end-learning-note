line":75,"column":15}}],"sources":{"app_1.js":"import './app_1.css'\nimport $ from 'jquery'\n// console.log($);\nimport Model from './base/Model'\nimport View from './base/View'\nimport EventBus from './base/EventBus'\n\n// const eventBus = new EventBus()\n\n// console.log(eventBus.on);\n// console.log(eventBus.trigger);\n\n// 数据相关都放到M\nconst m = new Model({\n    data: {\n        n: parseFloat(localStorage.getItem('n'))\n    },\n    update: function (data) {\n        Object.assign(m.data, data)\n        m.trigger('m_updated')\n        localStorage.setItem('n', m.data.n)\n    }\n})\n\n\n// 视图相关都放到V\n\n// 其他的都放到C\nconst init = (el)=> {\n    new View({\n        el: el,\n        data: m.data,\n        // eventBus: eventBus,\n        html: `\n        <!-- 有数据存储功能的计算器 -->\n        <div>\n            <div class=\"output\">\n                <span id=\"number\">{{n}}</span>\n            </div>\n            <div class=\"actions\">\n                <button id=\"add_1\">+1</button>\n                <button id=\"minus_1\">-1</button>\n                <button id=\"mul_1\">*2</button>\n                <button id=\"divide_1\">/2</button>\n            </div>\n        </div>\n        `,\n        render(data){\n            const n = data.n\n            if (this.el.children.length !== 0) this.el.empty()\n            $(this.html.replace('{{n}}', n))\n                .appendTo(this.el)\n        },\n        events: {\n            'click #add_1': 'add',\n            'click #minus_1': 'minus',\n            'click #mul_1': 'mul',\n            'click #divide_1': 'divide'\n        },\n        add() {\n            m.update({n: m.data.n + 1})\n        },\n        minus() {\n            m.update({n: m.data.n - 1})\n        },\n        mul() {\n            m.update({n: m.data.n * 2})\n        },\n        divide() {\n            m.update({n: m.data.n / 2})\n        },\n    })\n}\n\nexport default init"},"lineCount":null}},"error":null,"hash":"0b2dd2883b6900b2099d5f70614df79f","cacheData":{"env":{}}